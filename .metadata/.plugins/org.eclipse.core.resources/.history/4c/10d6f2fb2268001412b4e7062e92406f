import java.awt.Robot;
import java.awt.event.KeyEvent;


public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Robot robot = new Robot();
		new Thread(new Runnable(){
			public void run(){
				
				robot.keyPress(KeyEvent.VK_0);
				try{
					Thread.sleep(100);
				}catch(Exception e){
					
				}
				
			}
		}).start();
		
	}

}
